import java.util.Scanner;

public class multiplication {
    public static void main(string[] args){
        Scanner sc=new Scanner(System.in);
        int m,n;
        System.out.println("enter m: ");
        m=sc.nextInt();
        System.out.println("enter n: ");
        n=sc.nextInt();
        for(int i=1;i<=n;i++){
            System.out.println(i+"*"+m+"="+ i*m);

        }
    }
}

